{
  "css.validate": false,
  "editor.rulers": [80],
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "json",
    "html"
  ],
  "files.autoSave": "off",
  "stylelint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
